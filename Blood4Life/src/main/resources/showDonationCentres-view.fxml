    <stylesheets>
        <URL value="@/styles/backgrounds-style.css" />
        <URL value="@/styles/tables-style.css" />
    </stylesheets>
    <children>
      <TableView fx:id="donationCentresTableView" layoutX="14.0" layoutY="14.0" prefHeight="375.0" prefWidth="574.0">
