          <TableColumn fx:id="nameTableColumn" prefWidth="62.0" text="Name" />
          <TableColumn fx:id="maximumCapacityTableColumn" prefWidth="120.0" text="Maximum capacity" />
            <TableColumn fx:id="openHourTableColumn" prefWidth="84.0" text="Open hour" />
            <TableColumn fx:id="closeHourTableColumn" prefWidth="75.0" text="Close hour" />
            <TableColumn fx:id="countyTableColumn" prefWidth="59.0" text="County" />
            <TableColumn fx:id="cityTableColumn" prefWidth="46.0" text="City" />
            <TableColumn fx:id="streetTableColumn" prefWidth="64.0" text="Street" />
            <TableColumn fx:id="numberTableColumn" prefWidth="63.0" text="Number" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
